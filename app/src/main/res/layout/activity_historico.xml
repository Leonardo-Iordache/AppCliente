<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mainscreen.HistoricoActivity">

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="338dp"
        android:layout_height="714dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/totalPedidosTextView"
            android:layout_width="match_parent"
            android:layout_height="68dp"
            android:text="Total pedidos: " />

        <TextView
            android:id="@+id/pedidosRecogidosTextView"
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:text="Pedidos recogidos: " />

        <TextView
            android:id="@+id/pedidosEntregadosTextView"
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:text="Pedidos Entregados: " />

        <TextView
            android:id="@+id/pedidosEnRepartoTextView"
            android:layout_width="match_parent"
            android:layout_height="53dp"
            android:text="Pedidos en reparto: " />

        <TextView
            android:id="@+id/totalLogrosTextView"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:text="Total Logros: " />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>